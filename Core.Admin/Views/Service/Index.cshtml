                                <i class="icon-layers2"></i> &nbsp; الخدمات

                                
                            </div>

                        </div>
                        <div class="card-body">

                            <div class="row gutters">
                                <div class="row">
                                    <div class="col-xl-4 col-lg col-md-4 col-sm-4 col-12">
                                        <div class="form-group">
                                            <label for="fullName">بحث من :</label>
                                            <input type="date" class="form-control" placeholder=" ">
                                        </div>

                                    </div>
                                    <div class="col-xl-4 col-lg col-md-4 col-sm-4 col-12">
                                        <div class="form-group">
                                            <label for="fullName">بحث الى  :</label>
                                            <input type="date" class="form-control" id="fullName" placeholder=" ">
                                        </div>

                                    </div>
                                    <div class="col-xl-4 col-lg-4 col-md-4 col-sm-4 col-12">
                                        <div class="form-group">
                                            <label for="fullName"> التصنيف حسب المستخدم:</label>
                                            <select class="form-control">

                                                <option>اختر التصنيف...</option>
                                                <option>الاول</option>
                                                <option>الاول</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 ">
                                        <div class="text-right">

                                            <a href="#" class="add_userbtn"><i class="icon-search"></i> بحث</a>
                                        </div>
                                    </div>
                                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12 mt-30">

                                        <div class="card">
                                            <div class="card-body">
                                                <div class="table-responsive">
                                                    <table class="table table-bordered table-striped">
                                                        <thead>
                                                            <tr>
                                                                <th>#</th>
                                                                <th>اسم الخدمة</th>
                                                                <th>اسم مقدم الخدمة</th>
                                                                <th>اسم العميل</th>
                                                                <th>السعر</th>
                                                                <th>تاريخ اضافة الخدمة</th>
                                                                <th>الصورة</th>

                                                                <th></th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr>
                                                                <td>1</td>
                                                                <td></td>
                                                                <td></td>
                                                                <td></td>
                                                                <td>100$</td>
                                                                <td></td>
                                                                <td><img class="img_table" src="/images/logo-2.png"></td>

                                                                @*<td>
                                                                    <div class="td-actions">
                                                                        <a class="active" href="#"><i class="icon-power_settings_new"></i> تفعيل</a>
                                                                        <a class="edit" href="#" title="تعديل"><i class="icon-pencil"></i>  تعديل </a>
                                                                        <a class="delete" href="#" title="حذف"><i class="icon-delete"></i> حذف</a>

                                                                    </div>
                                                                </td>*@
                                                            </tr>
                                                           

                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                        </div>

                    </div>




                </div>
